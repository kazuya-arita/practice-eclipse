package driver;

import company.SalesEmployee;

public class Driver {

	public static void main(String[] args) {
		SalesEmployee hayami = new SalesEmployee(1234, "速水", 1000000);	
		System.out.println();
		hayami.displayInfo();
		hayami.displaySalesAmt();
		
		SalesEmployee kawai = new SalesEmployee(9876, "河合");
		System.out.println();
		kawai.displayInfo();
		kawai.displaySalesAmt();
	}

}
