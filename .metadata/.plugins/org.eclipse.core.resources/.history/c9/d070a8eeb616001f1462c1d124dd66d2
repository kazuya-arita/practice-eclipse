package company;

public class PlanningEmployee extends AbsEmployee {
	private int planningCnt;
	private int okCnt;
	
	public PlanningEmployee(int id, String name, int planningCnt, int okCnt) {
		super(id, name, "");
		this.planningCnt = planningCnt;
		this.okCnt = okCnt;
	}
	
	public void displayInfo() {
		System.out.println();
	}
}
