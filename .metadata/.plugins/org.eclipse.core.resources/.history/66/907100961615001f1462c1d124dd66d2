package driver;

import store.SpecialProduct;

public class Driver {

	public static void main(String[] args) {
		SpecialProduct keybord = new SpecialProduct();
		
		keybord.setName("特価キーボード");
		keybord.setPrice(3600);
		keybord.setOrgPrice(4500);
		
		System.out.println(keybord.getName() + ":" + keybord.display() + "円");
		keybord.displayOrgPrice();

	}

}
